{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "text-wrap: balance",
          "text-wrap: pretty",
          "text-size-adjust",
          "forced-color-adjust",
          "forced-color-adjust: none",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "typescript-config/consistent-casing": "off",
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  }
}